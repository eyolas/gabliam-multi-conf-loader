    "@types/debug": "^0.0.30",
    "@types/js-yaml": "^3.10.1",
    "@types/lodash": "^4.14.85",
    "@types/node": "^9.3.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.10.2",
    "publish-please": "^2.3.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.6.2",
    "vrsource-tslint-rules": "^5.8.0"
