  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --write {src,test}/**/*.ts",
    "lint": "tslint --project tsconfig.json '{src,test}/**/*.ts'",
    "build": "rimraf lib && tsc -p tsconfig.build.json",
